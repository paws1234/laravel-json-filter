{
    "name": "pawsmedz/laravel-json-filter",
    "description": "Dynamic JSON filtering for Eloquent Builder with Laravel 9-12 & PHP 8.1-8.4 support (MySQL, PostgreSQL & SQLite).",
    "keywords": ["laravel", "laravel-12", "json", "eloquent", "filter", "query", "mysql", "postgresql", "sqlite"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rayvand Jasper Valle Medrano",
            "email": "pawsmedz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.6|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Pawsmedz\\JsonFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pawsmedz\\JsonFilter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pawsmedz\\JsonFilter\\JsonFilterServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test:coverage": "vendor/bin/phpunit --coverage-html coverage/html --coverage-text",
        "test:laravel-12": "vendor/bin/phpunit --colors=always --group laravel-12",
        "analyze": "vendor/bin/phpstan analyse src tests --level=5",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
