{
    "name": "pawsmedz/laravel-json-filter",
    "description": "Dynamic JSON filtering for Eloquent Builder (MySQL & PostgreSQL).",
    "keywords": ["laravel", "json", "eloquent", "filter", "query"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rayvand Jasper Valle Medrano",
            "email": "pawsmedz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.6|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Pawsmedz\\JsonFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pawsmedz\\JsonFilter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pawsmedz\\JsonFilter\\JsonFilterServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
