{
    "name": "pawsmedz/laravel-json-filter",
    "description": "Universal JSON/Document filtering for ANY database type - SQL, NoSQL, Graph databases and more.",
    "keywords": ["laravel", "json", "eloquent", "filter", "query", "mysql", "postgresql", "mongodb", "nosql", "document", "unified", "database-agnostic"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rayvand Jasper Valle Medrano",
            "email": "pawsmedz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.6|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Pawsmedz\\JsonFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pawsmedz\\JsonFilter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pawsmedz\\JsonFilter\\UnifiedJsonFilterServiceProvider"
            ]
        },
        "suggest": {
            "jenssegers/mongodb": "For MongoDB support",
            "laravel-mongodb/laravel-mongodb": "Alternative MongoDB package"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test:coverage": "vendor/bin/phpunit --coverage-html coverage/html --coverage-text"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
